    <style name="Theme.AnimeApp.PopupOverlay" parent="Theme.AnimeApp" />

    <style name="Theme.AnimeApp.NeverCutout" parent="@style/Theme.Base">

        <item name="android:navigationBarColor">@color/bg</item>

    </style>

    <style name="MyBottomNavigationView.TextAppearance" parent="TextAppearance.MaterialComponents.Caption">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="DialogTheme" parent="Theme.AnimeApp">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="windowActionBar">false</item>
        <item name="android:navigationBarColor">@color/status</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@drawable/shape_corner_16dp</item>
    </style>

    <style name="BottomNavigationView" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textFontWeight" tools:targetApi="p">500</item>
        <item name="titleEnabled">true</item>
        <!--        <item name="iconTintActive">@color/basic_color</item>-->
        <!--        <item name="iconTintActive">@color/basic_color</item>-->
        <item name="contentInsetStartWithNavigation">0dp</item>
        <item name="labelVisibilityMode">selected</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="App.Custom.Indicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">?attr/colorOnBackground</item>
    </style>

    <style name="Theme.AnimeApp.Fullscreen" parent="Theme.AnimeApp">
        <item name="android:actionBarStyle">@style/Widget.Theme.AnimeApp.ActionBar.Fullscreen</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="ThemeOverlay.AnimeApp.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>

    <style name="Theme.AnimeApp.AppWidgetContainerParent" parent="@android:style/Theme.DeviceDefault">
        <!-- Radius of the outer bound of widgets to make the rounded corners -->
        <item name="appWidgetRadius">16dp</item>
        <!--
        Radius of the inner view's bound of widgets to make the rounded corners.
        It needs to be 8dp or less than the value of appWidgetRadius
        -->
        <item name="appWidgetInnerRadius">8dp</item>
    </style>

    <style name="Theme.AnimeApp.AppWidgetContainer" parent="Theme.AnimeApp.AppWidgetContainerParent">
        <!-- Apply padding to avoid the content of the widget colliding with the rounded corners -->
        <item name="appWidgetPadding">16dp</item>
    </style>

